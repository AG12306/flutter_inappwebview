<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/flutter_inappwebview_android_geckoview_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="true"
    android:focusable="true"
    android:focusableInTouchMode="true"
    tools:context="com.pichillilorenzo.flutter_inappwebview_android_geckoview.in_app_browser.InAppBrowserActivity">

    <com.pichillilorenzo.flutter_inappwebview_android_geckoview.pull_to_refresh.PullToRefreshLayout
        android:id="@+id/flutter_inappwebview_android_geckoview_pull_to_refresh"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.pichillilorenzo.flutter_inappwebview_android_geckoview.webview.in_app_webview.InAppWebView
            android:id="@+id/flutter_inappwebview_android_geckoview_webview"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </com.pichillilorenzo.flutter_inappwebview_android_geckoview.pull_to_refresh.PullToRefreshLayout>

    <ProgressBar
        android:id="@+id/flutter_inappwebview_android_geckoview_progress_bar"
        style="@android:style/Widget.Holo.ProgressBar.Horizontal"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:max="100"
        android:progress="0" />

</RelativeLayout>

